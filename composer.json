{
    "name": "kevinjohn/l5-boot-camp",
    "description": "A Laravel 5 platform, for creating Basecamp.com style applications.",
    "version": "0.0.1",
    "type": "platform",
    "keywords": ["platform", "framework", "basecamp"],
    "homepage": "https://github.com/Kevinjohn/L5-boot-camp",
    "time": "2015-04-01",
    "license": "MIT",
    "support": {
      "email": "l5-boot-camp@kevinjohngallagher.com",
      "issues": "https://github.com/Kevinjohn/L5-boot-camp/issues",
      "forum": "https://github.com/Kevinjohn/L5-boot-camp/issues",
      "wiki": "https://github.com/Kevinjohn/l5-boot-camp/wiki",
      "source": "http://github.com/Kevinjohn/L5-boot-camp"
    },
    "authors": [
        {
            "name": "Kevinjohn Gallagher",
            "email": "l5-boot-camp@kevinjohngallagher.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "5.0.*",
        "illuminate/html": "dev-master",
        "nesbot/carbon" : "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "illuminate/workbench": "dev-master",
        "behat/behat" : "~3",
        "squizlabs/php_codesniffer" : "~2",
        "barryvdh/laravel-debugbar" : "~2",
        "pragmarx/laravelcs" : "dev-master"
     },
    "suggest": {
      "monolog/monolog": "Advanced logging of the application flow",
      "gherkins/regexpbuilderphp": "Amazing Regex builder in English",
      "intouch/newrelic": "Why wouldn't you?",
      "intouch/laravel-newrelic": "Why wouldn't you?",
      "laracasts/generators": "I like these not being in Core, but they are useful.",
      "league/booboo": "Wonderful  error handling.",
      "raveren/kint/" : "Incredible debug framework!",
      "GrahamCampbell/Laravel-Markdown" : "Laravel 5 mardown",
      "Zizaco/entrust" : "Roles and responsibilities plugin",
      "CodeSleeve/laravel-stapler" : "Laravel 5 uploader",
      "barryvdh/laravel-dompdf" : "PDF generation",
      "sebastian/phpcpd" : "PHP Copy paste detector"
    },
    "autoload": {
        "psr-4": {
            "Kevinjohn\\L5BootCamp\\": "src/"
        },
        "psr-0": {
            "Kevinjohn\\L5BootCamp": "src/"
        }
    },
    "minimum-stability": "stable"
}
